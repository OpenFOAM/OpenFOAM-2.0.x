FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     simpleFoam;

startFrom       latestTime;

startTime       0;

stopAt          nextWrite;

endTime         500;

deltaT          1;

writeControl    timeStep;

writeInterval   1;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression compressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

libs
(
    "libOpenFOAM.so"
    "libcompressibleTurbulenceModels.so"
    "libincompressibleRASModels.so"
);

functions
{
    #include "readFields"
    #include "streamLines"
    #include "cuttingPlane"
    #include "forceCoeffs"
}


